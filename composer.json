{
    "name": "bhagat/laravel-saml2",
    "description": "A Laravel package for Saml2 integration as a SP (service provider) for multiple IdPs, based on OneLogin toolkit which is much more lightweight than simplesamlphp.",
    "keywords": ["laravel","saml", "saml2", "onelogin","adfs","active directory federation service"],
    "license": "MIT",
    "version": "1.1.0",
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0"
    },
   
    "autoload": {
        "psr-0": {
            "Bhagat\\LaravelSaml2\\": "src/"
        }
    },
    "authors": [
        {
            "name": "bhagat-singh",
            "email": "bhagatsingh.singh602@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Bhagat\\LaravelSaml2\\Saml2ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5.0",
        "ext-openssl": "*",
        "illuminate/support": ">=5.0.0",
        "onelogin/php-saml": "^3.0.0"
    }
}
